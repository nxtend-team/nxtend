{
  "$schema": "../../node_modules/@angular-devkit/schematics/collection-schema.json",
  "name": "Nxtend Ionic Angular",
  "version": "0.0.1",
  "schematics": {
    "init": {
      "factory": "./src/schematics/init/schematic",
      "schema": "./src/schematics/init/schema.json",
      "description": "Initialize the @nxtend/ionic-angular plugin",
      "aliases": ["ng-add"]
    },
    "application": {
      "factory": "./src/schematics/application/schematic",
      "schema": "./src/schematics/application/schema.json",
      "aliases": ["app"],
      "description": "Create an application"
    }
  }
}
